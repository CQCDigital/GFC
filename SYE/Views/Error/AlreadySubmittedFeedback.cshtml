@using SYE.ViewModels
@inject Microsoft.Extensions.Options.IOptions<ApplicationSettings> _config
@{
    var config = _config.Value;
    ViewData["Title"] = "Already submitted feedback" + _config.Value.SiteTextStrings.SiteTitleSuffix;
}

<gds-row>
    <gds-column desktop-size="TwoThirds" tablet-size="FullWidth">

        <gds-heading heading-type="H1" text="You've already submitted your feedback"></gds-heading>

        <gds-paragraph>
            It looks like you're trying to go back to a previous page. Once you've submitted your form, you cannot open it again.
        </gds-paragraph>
        <gds-paragraph>
            If you want to add more feedback or give feedback about a different service,
            you'll need to <gds-link class="govuk-link" link-text="go back to the start" href="@config.GFCUrls.StartPage"></gds-link>
            and submit a new form.
        </gds-paragraph>

        <gds-heading heading-type="H2" text="Get help with this form"></gds-heading>

        <gds-paragraph>
            You can get help on the phone if it's hard for you to do things online.
        </gds-paragraph>

    </gds-column>
</gds-row>